{
  "name": "tree-sitter-zpl",
  "version": "0.1.0",
  "description": "Tree-sitter Parser for Zebra Programming Language",
  "main": "bindings/node",
  "scripts": {
    "test": "tree-sitter test",
    "generate": "tree-sitter generate",
    "install": "node-gyp rebuild"
  },
  "author": "Chris Moore <chris@knowndecimal.com>",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.18.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8"
  },
  "tree-sitter": [
    {
      "scope": "source.zpl",
      "file-types": [
        "zpl"
      ],
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ],
  "gypfile": true,
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdmwebs/tree-sitter-zpl.git"
  },
  "keywords": [
    "parser"
  ],
  "bugs": {
    "url": "https://github.com/cdmwebs/tree-sitter-zpl/issues"
  },
  "homepage": "https://github.com/cdmwebs/tree-sitter-zpl#readme"
}
